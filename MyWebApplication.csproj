    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>262560e3-408f-4603-b85b-19b50adf2e97</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.0.1" />
